6. 
var p1 = {
fname: 'San',
getUserName: function(){
	return this.fname;
}
}
var p2 = p1;
p2.fname = 'John';
p2.lname = false;
console.log(p2.fname); // John
(function(){
console.log(p1.fname); // John
console.log(p1.lname); // false
  
console.log(p2.fname); // John
  console.log(p1.lname); // false
}())
