8.
var a;
let a = 300;
console.log(a); // Identifier 'a' has already been declared
_________
let a;
var  a=300;
console.log(a); // Identifier 'a' has already been declared
____________
a = 100;
let a;
console.log(a); // ReferenceError: Cannot access 'a' before initialization.
__________
a = 100;
var a;
console.log(a); // 100
